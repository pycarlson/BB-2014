<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Company</th>
      <th>Drop location</th>
      <th>Drop date</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.street %></td>
        <td><%= user.city %></td>
        <td><%= user.state %></td>
        <td><%= user.zip %></td>
        <td><%= user.phone %></td>
        <td><%= user.company %></td>
        <td><%= user.drop_location_id %></td>
        <td><%= user.drop_date_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

